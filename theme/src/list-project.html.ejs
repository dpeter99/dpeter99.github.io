<main>
    <div>
        <header>
            <h1><%= meta.title %></h1>
        </header>
        <!-- "{{.Content}}" pulls from the markdown content of the corresponding _index.md -->
        <%- content %>

        <ul class="cards-list">
            <% pipeline.files.filter((d)=>ArticleHelper.subArticle(d)).forEach(doc => {%>
                
                <project-card>
                    <project-title title="<%= doc.meta.title %>"></project-title>
                </project-card>
                
            <% }) %>
        </ul>
    </div>


</main>